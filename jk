def usg_find(number1,number2):

    max_number = max(number1,number2)
    usg = 0
    i = 1

    while i <= max_number:
        if number1 % i == 0 and number2 % i == 0:
            usg = i
        i += 1


    return usg

print(usg_find(10,10))



