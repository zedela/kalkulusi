d = {10:20,11:25,12:30,13:35,14:40}
def dictionary(dict):

    sum = 0
    for key in dict.keys():
        if key%2==0:
            sum += dict[key]
    dict['sum'] = sum
    return dict
print(dictionary(d))

__________________________
d = {10:20,11:25,12:30,13:35,14:40}
def dictionary(dict):
    list = []
    sum = 0
    for key in dict.keys():
        if key%2==1:
            list.append(dict[key])
    a = max(list)
    dict['sum'] = a
    return dict
print(dictionary(d))

